<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/volunteer_nav_graph"
    app:startDestination="@id/searchRequestFragment2">

    <fragment
        android:id="@+id/acceptedRequestsFragment2"
        android:name="com.example.pomocnysasiad.fragment.AcceptedRequestsFragment"
        android:label="fragment_accepted_requests"
        tools:layout="@layout/fragment_accepted_requests" >
        <action
            android:id="@+id/action_acceptedRequestsFragment2_to_acceptedRequestDetailsFragment"
            app:destination="@id/acceptedRequestDetailsFragment"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/from_right"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/from_right" />
    </fragment>
    <fragment
        android:id="@+id/accountFragment2"
        android:name="com.example.pomocnysasiad.fragment.AccountFragment"
        android:label="fragment_account"
        tools:layout="@layout/fragment_account" />
    <fragment
        android:id="@+id/searchRequestFragment2"
        android:name="com.example.pomocnysasiad.fragment.SearchRequestFragment"
        android:label="fragment_search_request"
        tools:layout="@layout/fragment_search_request" >
        <action
            android:id="@+id/action_searchRequestFragment2_to_requestDetails"
            app:destination="@id/requestDetails"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/from_right"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/from_right"
            app:popUpTo="@id/searchRequestFragment2"
            app:popUpToInclusive="false" />
    </fragment>
    <fragment
        android:id="@+id/requestDetails"
        android:name="com.example.pomocnysasiad.fragment.RequestDetails"
        android:label="fragment_request_details"
        tools:layout="@layout/fragment_request_details" >
        <action
            android:id="@+id/action_requestDetails_to_acceptedRequestsFragment2"
            app:destination="@id/acceptedRequestsFragment2"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/from_right"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/from_right" />
    </fragment>
    <fragment
        android:id="@+id/acceptedRequestDetailsFragment"
        android:name="com.example.pomocnysasiad.fragment.AcceptedRequestDetailsFragment"
        android:label="AcceptedRequestDetailsFragment" />
</navigation>